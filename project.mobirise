div class=\"row\">\n\n\n        <div class=\"col-12 col-sm-4 col-lg\">\n\n            <div class=\"wrapper\">\n                <div class=\"card-wrapper\">\n\n                    <img src=\"@PROJECT_PATH@/assets/images/1.png\" class=\"img-responsive clients-img\">\n                    <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .iconfont-wrapper\">Comma</h5>\n\n                </div>\n\n            </div>\n\n        </div>\n        <div class=\"col-12 col-sm-4 col-lg\">\n\n            <div class=\"wrapper\">\n                <div class=\"card-wrapper\">\n\n                    <img src=\"@PROJECT_PATH@/assets/images/2.png\" class=\"img-responsive clients-img\">\n                    <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .iconfont-wrapper\">Linkup</h5>\n\n                </div>\n\n            </div>\n\n        </div>\n        <div class=\"col-12 col-sm-4 col-lg\">\n\n            <div class=\"wrapper\">\n                <div class=\"card-wrapper\">\n\n                    <img src=\"@PROJECT_PATH@/assets/images/3.png\" class=\"img-responsive clients-img\">\n                    <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .iconfont-wrapper\">Square</h5>\n\n                </div>\n\n            </div>\n\n        </div>\n        <div class=\"col-12 col-sm-4 col-lg\">\n\n            <div class=\"wrapper\">\n                <div class=\"card-wrapper\">\n\n                    <img src=\"@PROJECT_PATH@/assets/images/4.png\" class=\"img-responsive clients-img\">\n                    <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .iconfont-wrapper\">Triangle</h5>\n\n                </div>\n\n            </div>\n\n        </div>\n        <div class=\"col-12 col